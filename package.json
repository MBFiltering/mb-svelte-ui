{
	"name": "@mbsmart/ui",
	"version": "0.1.1",
	"private": true,
	"description": "MB Smart UI component library for SvelteKit",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/MBFiltering/mb-svelte-ui.git"
	},
	"scripts": {
		"build": "svelte-package -i src",
		"prepare": "npm run build",
		"prepublishOnly": "npm run build"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./styles.css": "./dist/styles.css",
		"./atoms": {
			"types": "./dist/components/atoms/index.d.ts",
			"svelte": "./dist/components/atoms/index.js",
			"default": "./dist/components/atoms/index.js"
		},
		"./molecules": {
			"types": "./dist/components/molecules/index.d.ts",
			"svelte": "./dist/components/molecules/index.js",
			"default": "./dist/components/molecules/index.js"
		},
		"./organisms": {
			"types": "./dist/components/organisms/index.d.ts",
			"svelte": "./dist/components/organisms/index.js",
			"default": "./dist/components/organisms/index.js"
		},
		"./templates": {
			"types": "./dist/components/templates/index.d.ts",
			"svelte": "./dist/components/templates/index.js",
			"default": "./dist/components/templates/index.js"
		},
		"./utils": {
			"types": "./dist/utils/index.d.ts",
			"default": "./dist/utils/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^5.0.0",
		"@lucide/svelte": ">=0.400.0"
	},
	"devDependencies": {
		"@sveltejs/package": "^2.3.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"svelte": "^5.39.5",
		"@lucide/svelte": "^0.552.0"
	},
	"keywords": [
		"svelte",
		"sveltekit",
		"components",
		"ui",
		"tailwindcss"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts"
}
